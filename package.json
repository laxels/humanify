{
  "name": "humanify",
  "version": "2.2.2",
  "description": "",
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build --compile --minify --sourcemap --bytecode src/index.ts --outfile dist/humanify",
    "validate": "bun run typecheck && bun run test:unit && bun run lint",
    "typecheck": "tsc --noEmit",
    "test:unit": "bun test ./src/**/*.test.ts",
    "test:e2e": "bun run build && bun test ./src/**/*.test.e2e.ts",
    "lint": "biome check --write"
  },
  "bin": {
    "humanify": "dist/humanify"
  },
  "type": "module",
  "keywords": [
    "humanify",
    "decompiler",
    "decompile",
    "unobfuscate",
    "unobfuscator",
    "deobfuscate",
    "deobfuscator",
    "decompile",
    "decompiler",
    "reverse engineering",
    "unminify",
    "unuglify",
    "llm",
    "anthropic",
    "claude"
  ],
  "author": "Jesse Luoto",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@babel/core": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@dotenvx/dotenvx": "^1.51.1",
    "babel-plugin-transform-beautifier": "^0.1.1",
    "commander": "^14.0.2",
    "lodash": "^4.17.21",
    "webcrack": "^2.15.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/babel__core": "^7.20.5",
    "@types/bun": "latest",
    "@types/lodash": "^4.17.21",
    "typescript": "^5.9.3"
  },
  "private": true
}
