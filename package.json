{
  "name": "humanifyjs",
  "version": "2.2.2",
  "description": "",
  "main": "dist/index.mjs",
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "pkgroll",
    "validate": "bun run typecheck && bun run test:unit && bun run lint",
    "typecheck": "tsc --noEmit",
    "test:unit": "bun test ./src/**/*.test.ts",
    "test:e2e": "bun run build && bun test ./src/**/*.test.e2e.ts",
    "lint": "biome check --write"
  },
  "bin": {
    "humanify": "dist/index.mjs"
  },
  "type": "module",
  "keywords": [
    "humanify",
    "decompiler",
    "decompile",
    "unobfuscate",
    "unobfuscator",
    "deobfuscate",
    "deobfuscator",
    "decompile",
    "decompiler",
    "reverse engineering",
    "unminify",
    "unuglify",
    "llm",
    "anthropic",
    "claude"
  ],
  "author": "Jesse Luoto",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "0.39.0",
    "@babel/core": "^7.25.2",
    "@babel/types": "^7.25.2",
    "@dotenvx/dotenvx": "^1.31.2",
    "babel-plugin-transform-beautifier": "^0.1.0",
    "commander": "^13.0.0",
    "webcrack": "^2.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/babel__core": "^7.20.5",
    "@types/bun": "latest",
    "pkgroll": "^2.4.2",
    "typescript": "^5.5.4"
  }
