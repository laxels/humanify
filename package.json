{
  "name": "humanifyjs",
  "version": "2.2.2",
  "description": "",
  "main": "dist/index.mjs",
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "pkgroll",
    "validate": "bun run typecheck && bun run test && bun run lint && bun run format",
    "typecheck": "tsc --noEmit",
    "test": "bun run test:unit && bun run test:e2e",
    "test:unit": "bun test ./src/**/*.test.ts",
    "test:e2e": "bun run build && bun test ./src/**/*.test.e2e.ts",
    "lint": "eslint --fix src/**/*",
    "format": "prettier --write --list-different src/**/*"
  },
  "bin": {
    "humanify": "dist/index.mjs"
  },
  "type": "module",
  "keywords": [
    "humanify",
    "decompiler",
    "decompile",
    "unobfuscate",
    "unobfuscator",
    "deobfuscate",
    "deobfuscator",
    "decompile",
    "decompiler",
    "reverse engineering",
    "unminify",
    "unuglify",
    "llm",
    "anthropic",
    "claude"
  ],
  "author": "Jesse Luoto",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "0.39.0",
    "@babel/core": "^7.25.2",
    "@babel/types": "^7.25.2",
    "@dotenvx/dotenvx": "^1.31.2",
    "@types/babel__core": "^7.20.5",
    "babel-plugin-transform-beautifier": "^0.1.0",
    "commander": "^13.0.0",
    "typescript": "^5.5.4",
    "webcrack": "^2.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/bun": "latest",
    "eslint": "^9.8.0",
    "eslint-plugin-unused-imports": "^4.1.2",
    "globals": "^16.0.0",
    "pkgroll": "^2.4.2",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.0.0"
  }
}
